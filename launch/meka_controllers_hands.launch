<launch>

<rosparam file="$(find m3ros_control)/config/hardware_controllers.yaml" command="load" />

<!-- hand controllers -->
<group ns="meka_roscontrol" >
  <node pkg="controller_manager" type="spawner" output="screen" name="hand_controllers" args="--shutdown-timeout=2.0 right_hand_position_trajectory_controller left_hand_position_trajectory_controller"/>
</group>


</launch>

