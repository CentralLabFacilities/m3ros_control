<launch>

<rosparam file="$(find m3ros_control)/config/hardware_controllers.yaml" command="load" />
<arg name="cmd" default="start"/>

<!-- stiffness controller -->
<group ns="meka_roscontrol" >
  <node pkg="controller_manager" type="controller_manager" output="screen" name="meka_stiffness_ctrl" args="$(arg cmd) stiffness_controller"/>
</group>
</launch>

